{
    "name": "stfn/laravel-pending-updates",
    "description": "",
    "keywords": [
        "stfndamjanovic",
        "laravel",
        "laravel-pending-updates"
    ],
    "homepage": "https://github.com/stfndamjanovic/laravel-pending-updates",
    "license": "MIT",
    "authors": [
        {
            "name": "Stefan Damjanovic",
            "email": "damjanovicstefan0@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/laravel-package-tools": "^1.14.0",
        "illuminate/contracts": "^9.0|^10.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^6.0",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^7.0|^8.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5",
        "spatie/pest-plugin-test-time": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Stfn\\PendingUpdates\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Stfn\\PendingUpdates\\Tests\\": "tests"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
              "Stfn\\PendingUpdates\\PendingUpdateServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
